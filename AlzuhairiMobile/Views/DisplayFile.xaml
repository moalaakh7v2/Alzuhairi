             x:Class="AlzuhairiMobile.Views.DisplayFile">
    <ContentPage.Content>
        <StackLayout>
        <o:VideoPlayer x:Name="vidDisplay" AutoPlay="True" FillMode="ResizeAspectFill" />
        <WebView Source="" x:Name="pdfDisplay" HorizontalOptions="FillAndExpand" VerticalOptions="FillAndExpand" />
        </StackLayout>
    </ContentPage.Content>
</ContentPage>